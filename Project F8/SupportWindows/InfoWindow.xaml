        <TextBox x:Name="nameTextBox" HorizontalAlignment="Left" Height="21" Margin="116,22,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="282"/>
        <TextBox x:Name="idTextBox" HorizontalAlignment="Left" Height="21" Margin="116,53,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="49"/>
        <TextBox x:Name="discriptionTextBox" HorizontalAlignment="Left" Height="101" Margin="116,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="282"/>
